@model List<MyProject.Models.TableViewModels.CatalogTableVM>
    @{
        ViewBag.Title = "Request Book";
    }

    <h2>Catalog</h2>

    <div class="row">
        <div class="col-lg-12">
            <table class="table table-bordered">
                <tr>
                    <th>#</th>
                    <th>Author</th>
                    <th>Title</th>
                    
                </tr>

                @foreach (var element in Model)
                {
                <tr>
                    <td>@element.Id</td>
                    <td>@element.Author </td>
                    <td>@element.Title </td>
                    <td>
                    <input onclick="document.location.href = '@Url.Content("~/RequestBook/Select/"+element.ClassId)'" type="button" value="See " class="btn btn-default" />
                    </td>
                 </tr>
                }
            </table>
        </div>
    </div>
